<script>
    function mediaSelection(params, mediaQuality = "medium") {
        if (Array.isArray(params)) {
            console.log("array");
            params = params[0];
        }
        let mediaURL = "";
        if (params && params.formats) {
            switch (mediaQuality) {
                case "large":
                    if (params.formats.large) {
                        mediaURL = params.formats.large.url;
                        break;
                    }
                case "medium":
                    if (params.formats.medium) {
                        mediaURL = params.formats.medium.url;
                        break;
                    }
                case "small":
                    if (params.formats.small) {
                        mediaURL = params.formats.small.url;
                        break;
                    }
                case "thumbnail":
                    if (params.formats.thumbnail) {
                        mediaURL = params.formats.thumbnail.url;
                        break;
                    }
            }
        }
        return mediaURL;
    }
    function getSlideTemplate(params, index = "") {
        return `<div class="carousel-item ${Number(index) == 0 ? 'active' : ''}">
                            <img class="fadeIn d-block rounded img-fluid" src="${mediaSelection(params.slide_img)}" alt="First slide">
                            <div class="d-none d-md-block carousel-caption py-0"
                                style="background-color: var(--blue-transparent);">
                                <h5>${params.slide_title}</h5>
                                <p>${params.slide_text}</p>
                            </div>
                        </div>`
    }
    function getIndicators(index) {
        console.log("sdsd");
        return `<li data-target="#carouselExampleIndicators" data-slide-to="${index}" class="${Number(index) == 0 ? 'active' : ''}"></li>`;

    }
    function slideOverlay(index) {
        index -= 1;
        console.log(index);
        document.body.scrollTop = 0; // For Safari
        document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE and Opera
        var results = $("#inner");
        var indicators = $("#indicators");
        indicators.empty();
        results.empty();
        $('#overlay').addClass('slideShowView');
        let toAppend = [];
        let controls = []
        slides[index].slides.forEach((s, i) => {
            toAppend.push(getSlideTemplate(s, i));
            controls.push(getIndicators(i));
        });
        indicators.append(controls);
        results.append(toAppend.join(""));

    }

    function showblocks(params) {
        let blocks = document.getElementsByClassName('dropdown');
        let element = document.getElementById('explore');
        element.classList.add('animated');
        element.classList.add('moveback');
        for (const b of blocks) {
            b.classList.add('animated');
        }
        console.log(blocks.length);

    }

    function closeSlideShow() {
        document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE and Opera
        $('#overlay').removeClass('slideShowView');

    }</script>

