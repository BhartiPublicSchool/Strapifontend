{
    "name": "bps-frontend",
    "main": "./src/app.ts",
    "scripts": {
        "start": "node ./dist/src/app.js --production",
        "start:prod": "concurrently \"tsc --watch\" \"node ./dist/src/app.js --production\"",
        "startDev": "concurrently \"tsc --watch\" \"nodemon ./dist/src/app.js\"",
        "build": "npx tsc",
        "swagger": "node ./swagger.js",
        "test": "ts-node node_modules/jest/bin/jest.js --no-cache",
        "test:i": "yarn test:unit --runInBand ; yarn test:integration:skip:coverage",
        "lint": " ./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx",
        "start:dev": "concurrently \"yarn start:devServer\" \"yarn check-types\"",
        "start:devServer": "nodemon -w ./src",
        "check-types": "tsc --watch"
    },
    "pre-commit": [
        "lint"
    ],
    "dependencies": {
        "app-module-path": "^2.2.0",
        "axios": "^1.7.6",
        "axios-retry": "^4.5.0",
        "compression": "^1.7.4",
        "cookie": "^1.0.2",
        "cookie-parser": "^1.4.6",
        "cron-parser": "^4.9.0",
        "dotenv": "^16.4.5",
        "ejs": "^3.1.10",
        "express": "^4.19.2",
        "express-correlation-id": "^3.0.1",
        "express-session": "^1.18.0",
        "express-validation": "^4.1.0",
        "express-validator": "^7.1.0",
        "helmet": "^7.1.0",
        "http-codes": "^1.0.0",
        "http-headers": "^3.0.2",
        "joi": "^17.13.1",
        "markdown-it": "^14.1.0",
        "markdown-it-attrs": "^4.3.1",
        "markdown-it-classy": "^0.2.0",
        "markdown-it-underline": "^1.0.1",
        "node-cron": "^3.0.3",
        "nodemailer": "^6.10.0",
        "qs": "^6.14.0",
        "uuid": "^9.0.1",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.7",
        "@types/express-correlation-id": "^1.2.6",
        "@types/http-codes": "^1.0.4",
        "@types/jest": "^29.5.4",
        "@types/markdown-it": "^14.1.2",
        "@types/markdown-it-attrs": "^4.1.3",
        "@types/nodemailer": "^6.4.17",
        "@types/uuid": "^9.0.8",
        "concurrently": "^8.2.1",
        "eslint": "^8.45.0",
        "jest": "^29.6.4",
        "nodemon": "^3.0.1",
        "pre-commit": "^1.0.10",
        "swagger-autogen": "^2.23.5",
        "ts-jest": "^29.2.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    }
}
